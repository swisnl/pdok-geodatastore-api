{
    "name": "swisnl/pdok-geodatastore-api",
    "type": "library",
    "description": "A client for the PDOK Geodatastore API.",
    "keywords": [
        "swisnl",
        "swis",
        "publieke-dienstverlening-op-de-kaart",
        "pdok",
        "geodatastore",
        "nationaal-georegister",
        "ngr",
        "api",
        "client"
    ],
    "homepage": "https://github.com/swisnl/pdok-geodatastore-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Jasper Zonneveld",
            "email": "jasper@swis.nl",
            "homepage": "https://www.swis.nl/over-ons/jasper-zonneveld",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "psr/http-message": "^1.0",
        "php-http/httplug": "^1.1",
        "php-http/discovery": "^1.3",
        "php-http/client-implementation": "^1.0",
        "php-http/client-common": "^1.6",
        "php-http/multipart-stream-builder": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "php-http/guzzle6-adapter": "^1.0",
        "guzzlehttp/psr7": "^1.2"
    },
    "scripts": {
        "test": "phpunit"
    },
    "autoload": {
        "psr-4": {
            "Swis\\PdokGeodatastoreApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Swis\\PdokGeodatastoreApi\\Tests\\": "tests/"
        }
    }
}
